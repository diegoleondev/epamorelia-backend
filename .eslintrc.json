{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["standard-with-typescript", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": ["dist/", "node_modules/", "tsconfig.json"],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "off"
  }
}
